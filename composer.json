{
    "name": "marufmax/deja-vu",
    "description": "Caching with redis sentinal, master and slave. Supports PHPRedis client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maruf Alom",
            "email": "desk.maruf@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^v7.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3.0"
    },
    "autoload": {
        "psr-4": {
            "MarufMax\\DejaVu\\": "src"
        }
    },
    "autoload-dev": {
